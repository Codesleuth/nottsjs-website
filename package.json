{
  "name": "nottsjs-website",
  "private": true,
  "description": "The NottsJS website",
  "version": "0.0.1",
  "author": "David Wood <david.p.wood@gmail.com>",
  "dependencies": {
    "gatsby": "^2.20.8",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "remark-html": "^11.0.1"
  },
  "devDependencies": {
    "prettier": "^2.0.2",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "nottsjs",
    "website"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo 'No tests' && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nottsjs/website"
  },
  "bugs": {
    "url": "https://github.com/nottsjs/website/issues"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always"
  }
}
